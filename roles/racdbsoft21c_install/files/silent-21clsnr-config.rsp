---
# =====================================================================================
# Install Oracle Database 21.3.0 software for RAC using Ansible:
# Defining silent response file for listener configuration
# =====================================================================================
[GENERAL]
RESPONSEFILE_VERSION="21.0"
CREATE_TYPE="CUSTOM"
[oracle.net.ca] 
INSTALLED_COMPONENTS={"server","net8","javavm"} 
INSTALL_TYPE=""typical"" 
LISTENER_NUMBER=1 
LISTENER_NAMES={"LISTENER"} 
LISTENER_PROTOCOLS={"TCP;1523"} 
LISTENER_START=""LISTENER"" 
NAMING_METHODS={"TNSNAMES","ONAMES","HOSTNAME"} 
NSN_NUMBER=1 
NSN_NAMES={"EXTPROC_CONNECTION_DATA"} 
NSN_SERVICE={"PLSExtProc"} 
NSN_PROTOCOLS={"TCP;HOSTNAME;1523"}
